{
  "name": "controllerly-ui",
  "version": "0.0.6",
  "description": "Intuitive Smartphone Gamepad User Interface for Controllerly.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Marco Klein",
  "license": "MIT",
  "keywords": [
    "Gamepad",
    "User Interface",
    "Remote",
    "WebRTC",
    "Typescript",
    "Game",
    "Development",
    "Smartphone"
  ],
  "bugs": {
    "url": "https://github.com/marcoklein/controllerly-ui/issues"
  },
  "homepage": "https://github.com/marcoklein/controllerly-ui#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
