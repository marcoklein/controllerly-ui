    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoklein/controllerly-ui.git"
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "bootstrap": "^4.3.1",
